<Page x:Class="SPIDVerificationAPI_WPF_Sample.CreateProfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SPIDVerificationAPI_WPF_Sample"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="CreateProfile">

    <Grid>
        <Button x:Name="createBtn" Content="Create Profile" HorizontalAlignment="Left" Margin="25,129,0,0" VerticalAlignment="Top" Width="249" Height="30" Click="createBtn_Click"/>
        <TextBox x:Name="speakerTxt" HorizontalAlignment="Left" Height="23" Margin="25,101,0,0" TextWrapping="Wrap" Text="SpeakerId" VerticalAlignment="Top" Width="249"/>

    </Grid>
</Page>
